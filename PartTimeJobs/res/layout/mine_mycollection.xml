<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:background="#FFFFFF"
              >
    <RelativeLayout android:layout_height="50dp"
                    android:background="@color/title_blue"
                    android:layout_width="match_parent">
         <ImageView android:src="@drawable/back_button"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_width="60dp"
                        android:layout_height="23dp"
                        android:id="@+id/mine_collection_back"
                        android:layout_marginLeft="10dp"></ImageView>
        <TextView android:text="我的收藏"
                  android:textSize="20dp"
                  android:textColor="#FFFFFF"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_centerInParent="true"></TextView>
    </RelativeLayout>
    <ListView android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:id="@+id/mine_mycollection_list"
              ></ListView>
    
